package arr2;

public class Arr2 {
	
	public static void main(String[] args) {
		int a2[][] = {
				{5, 5, 5, 5, 5},
				{10, 10, 10, 10, 10},
				{20, 20, 20, 20, 20},
				{30, 30, 30, 30, 30},
				{40, 40, 40, 40, 40}
		};
		int total = 0;
		float average = 0;
		
		for (int i=0; i< a2.length; i++) {
			for (int j=0; j < a2[i].length; j++) {
				total += a[i][j];
				average +=1;
			}
		}
		average = (float)total/average;

		System.out.println("Total: "+total);
		System.out.println("Average: "+average);
	}

}
