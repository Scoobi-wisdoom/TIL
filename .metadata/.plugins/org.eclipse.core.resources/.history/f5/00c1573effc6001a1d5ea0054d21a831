package ifelse;

import java.util.Random;

public class If1 {

	public static void main(String[] args) {
		Random r = new Random();
		int num = r.nextInt(20) + 1;
		if (num < 5 || num > 10) {
			System.out.println(x=3);
			return;
		}
		System.out.println("Program Start ...");
		System.out.println("Result: "+num);
