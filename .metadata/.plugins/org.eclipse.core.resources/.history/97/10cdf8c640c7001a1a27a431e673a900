package op;

public class Op5 {

	public static void main(String[] args) {
		char c = 'A';
		boolean result = ('0' <= c && c <= '9');
		System.out.println(result);
		
		char d = 'A';
		boolean result1 = ('a' <= d && d <= 'z') || ('A' <= d && d <= 'Z');
		System.out.println(result1);
		
		byte b =10;
		char ch = 'A';
		int i = 100;
		long l = 1000L;
		
		b = (byte)i;
		ch = (char)b;
		short s = (short)ch;
		float f = l;
		i = ch;
		
	}

}
