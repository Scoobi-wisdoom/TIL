package math;

import java.util.Random;

public class Ma1 {

	public static void main(String[] args) {
		// 난수 생성
		// 0.0 <= x < 1.0
		double d = Math.random();
		System.out.println(d);
		// 1 ~ 10
		int num1 = (int)(Math.random() * 10) + 1;
		System.out.println(num1);
		
		// Random
		Random r = new Random();
		int num2 = r.nextInt(5);
		System.out.println(num2);
	}

}
