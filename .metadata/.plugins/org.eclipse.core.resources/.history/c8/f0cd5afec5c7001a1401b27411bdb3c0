package forwhile;

public class For4 {
	public static void main(String[] args) {
		for (int i=0; i<5;i++) {
			for (int j=0; j<5; j++) {
				if (i == 2 && j ==2) {
					break;					
				}
				if (i == j) {
					System.out.print("[i], [j] "+i+", "+j+" | ");
				}
				
			}
			System.out.println();
		}
	}
}
