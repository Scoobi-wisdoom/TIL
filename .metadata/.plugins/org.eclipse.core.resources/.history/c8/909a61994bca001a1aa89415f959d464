package car;

public class Car {
	String name;
	String color;
	int size;
	int gasSize;
	int gas;
	double mileage;
	
	public Car() {
		
	}
	public Car(String name, String color, int gasSize, int gas, double mileage) {
		this.name = name;
		this.color = color;
		this.size = 2000;
		this.gasSize = gasSize;
		this.mileage = mileage;
	}
}
