package forwhile;

import java.util.Scanner;

public class While3 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String number = "";
		System.out.println("Input A Number");
		number = sc.nextLine();
		System.out.println("Input Number: "+ number);
		System.out.println("Goodbye");

		sc.close();
	}
}
