package car;

public class Car {
	int serial;
	String name;
	double size;
	
	public Car() {
		
	}
	
	public Car(String name, double size) {
		super();
		this.name = name;
		this.size = size;
	}
	
	public Car(int serial, String name, double size) {
		this.serial = serial;
		this.name = name;
		this.size = size;
	}

	
	

}
