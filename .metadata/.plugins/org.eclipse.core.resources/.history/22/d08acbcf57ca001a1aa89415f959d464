package car;

public class Car2 {
	String name;
	String color;
	int size;
	double fuel;
	String scale;
	int passenger;
	double speed;
	boolean power;

	
	public Car2() {
		
	}
	public Car2(String name, String color) {
		this.name = name;
		this.color = color;
		this.size = 3000;
		this.fuel = 0;
		this.scale = "Big";
		this.passenger = 8;
		this.speed = 0;
		this.power = false;

	}
	
	public void powerOn() {
		this.power = true;
	}
	public void powerOff() {
		this.power = false;
	}
	public void speedUp() {
		
	}
	public void speedDown() {
		
	}
	public void fuelUp() {
		
	}
	public void fuelDown() {
		
	}
	

	
}
