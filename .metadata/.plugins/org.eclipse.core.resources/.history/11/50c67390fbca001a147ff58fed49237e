package calc;

public class Calc {
	
	
	public int add(int a, int b) {		
		int add = 0;
		add = a + b;		
		return add;
	}
	
	public double add(int a, double b) {
		double add = 0;
		add = a + b;
		return add;
	}
	
	public double add(double a, int b) {
		double add = 0;
		add = a + b;
		return add;
	}
	
	public double add(double a, double b) {
		double add = 0;
		add = a + b;
		return add;
	}

}
