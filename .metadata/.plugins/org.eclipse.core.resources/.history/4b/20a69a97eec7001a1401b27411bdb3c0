package str;

import java.util.Arrays;

public class Str3 {
	
	public static void main(String args[]) {
		String email = "jmlee@tonesol.com";
		// id 와 domain(tonesol)을 분리하여 스트링을 만드시오.
		// id 와 domain을 출력하시오.
		
		email.indexOf("@");
		
		char [] c = email.toCharArray();
		System.out.println(c);
	}

}
